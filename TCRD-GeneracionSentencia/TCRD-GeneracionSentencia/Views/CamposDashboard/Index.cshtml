<div class="container-fluid">

    <div class="row">
        <div class="col-lg-12">
            <!-- USER DATA-->
            <div class="user-data m-b-30">
                <h3 class="title-3 m-b-30">
                    <i class="zmdi zmdi-account-calendar"></i>Campos
                </h3>
                <div class="table-responsive table-data">
                    <table class="table">
                        <thead>
                            <tr>
                                <td>
                                </td>
                                <td>Nombre</td>
                                <td>Estado</td>
                                <td>Usuario</td>
                                <td>Fecha Creacion</td>
                                <td></td>
                            </tr>
                        </thead>
                        <tbody>

                            @{

                                foreach (var item in Model)
                                {
                                                        <tr>
                                                            <td>
                                                                <label class="au-checkbox">
                                                                    <input type="checkbox">
                                                                    <span class="au-checkmark"></span>
                                                                </label>
                                                            </td>
                                                            <td>
                                                                <div class="table-data__info">
                                                                    <h6>@item.Nombre</h6>

                                                                </div>
                                                            </td>
                                                            <td>
                                                                <span class="table-data__info">@item.Estatus</span>
                                                            </td>
                                                            <td>
                                                                <span class="table-data__info">@item.Usuario</span>
                                                            </td>
                                                            <td>
                                                                <span class="table-data__info">@item.Fecha_Registro</span>
                                                            </td>
                                                            <td>
                                                                @*<span class="more">
                <i class="zmdi zmdi-edit"></i>
            </span>*@

                                                                <span data-id="@item.ID" class="more delete-button">
                                                                </span>
                                                            </td>
                                                        </tr>
                                }
                            }


                        </tbody>
                    </table>
                </div>
            </div>
            <!-- END USER DATA-->
        </div>
    </div>
</div>


