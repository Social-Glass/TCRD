
@{
    ViewBag.Title = "Index";
}

<style>

    .btn-info {
        color: #fff;
        background-color: rgb(122, 34, 46);
        border-color: rgb(122, 34, 46);
    }
</style>

<div class="container-fluid">

    <div class="row">
        <div class="col-lg-12">
            <!-- USER DATA-->
            <div class="user-data m-b-30">
                <h3 class="title-3 m-b-30">
                    <i class="zmdi zmdi-account-calendar"></i>Datos del Dashboard
                </h3>
                <div class="table-responsive table-data">
                    <table class="table">
                        <thead>
                            <tr>
                                <td>
                                </td>
                                <td>Expedientes Recibidos</td>
                                <td>Expedientes en Proceso</td>
                                <td>Expedientes Despachados</td>
                                <td>Sentencias Generadas</td>
                                <td>Sentencias Publicadas</td>
                                <td>Usuario</td>
                                <td>Fecha</td>
                                <td></td>
                            </tr>
                        </thead>
                        <tbody>

                            @{

                                foreach (var item in Model)
                                {
                                    <tr>
                                        <td>
                                            <label class="au-checkbox">
                                                <input type="checkbox">
                                                <span class="au-checkmark"></span>
                                            </label>
                                        </td>
                                        <td>
                                            <div class="table-data__info">
                                                <h6>@item.exp_recibidos</h6>

                                            </div>
                                        </td>
                                        <td>
                                            <div class="table-data__info">
                                                <h6>@item.exp_proceso</h6>

                                            </div>
                                        </td>
                                        <td>
                                            <div class="table-data__info">
                                                <h6>@item.exp_despachados</h6>

                                            </div>
                                        </td>
                                        <td>
                                            <span class="table-data__info">@item.sen_generadas</span>
                                        </td>
                                        <td>
                                            <span class="table-data__info">@item.sen_publicadas</span>
                                        </td>
                                        <td>
                                            <span class="table-data__info">@item.usuario</span>
                                        </td>
                                        <td>
                                            <span class="table-data__info">@item.fecha_creacion</span>
                                        </td>
                                        <td>
                                            <span class="more">
                                                <i class="zmdi zmdi-more"></i>
                                            </span>
                                        </td>
                                    </tr>
                                }
                            }


                        </tbody>
                    </table>
                </div>
            </div>
            <!-- END USER DATA-->
        </div>
    </div>
</div>
